#include<stdio.h>
void namaste();
void bojour();
int main()
{
    char lancheck;
    printf("enter 1 for Indian or 2 for French: ");
   scanf("%c",&lancheck);
   if (lancheck=='i')
   {
      namaste();
   }
   else
   {
       bojour();

   }
   
return 0;
}
void namaste()
{
    printf("namaste");
}
void bojour()

{
    printf("bonjourooo");
}
